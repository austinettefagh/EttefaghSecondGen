<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_Validation_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 ISBLANK(Email),
 ISBLANK(Phone)
)</errorConditionFormula>
    <errorMessage>Please populate the email and phone.</errorMessage>
</ValidationRule>
